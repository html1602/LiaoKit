{
  "name": "@yuandezuohua/liaokit",
  "private": false,
  "version": "2.8.7",
  "description": "LiaoKit - 现代化Vue3组件库，支持AI智能消息适配和多窗口系统",
  "type": "module",
  "main": "./dist/liaokit.umd.cjs",
  "module": "./dist/liaokit.js",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/liaokit.js",
      "require": "./dist/liaokit.umd.cjs"
    },
    "./dist/style.css": "./dist/liaokit.css",
    "./dist/liaokit.css": "./dist/liaokit.css"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "vue3",
    "components",
    "ui-library",
    "typescript",
    "ai-adapter",
    "multi-window",
    "liaokit"
  ],
  "author": "LiaoKit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/html1602/LiaoKit.git"
  },
  "homepage": "https://github.com/html1602/LiaoKit#readme",
  "bugs": {
    "url": "https://github.com/html1602/LiaoKit/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:lib": "vite build --mode library && copy lib.d.ts dist\\lib.d.ts",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib",
    "generate:docs": "node tools/generate-docs.js",
    "generate:docs:ast": "node tools/generate-docs-ast.js"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@antv/g2plot": "^2.4.33",
    "@vueuse/motion": "^3.0.3",
    "marked": "^15.0.12",
    "markstream-vue": "^0.0.3-beta.6",
    "openai": "^5.7.0",
    "sass": "^1.89.1",
    "unplugin-icons": "^22.1.0",
    "vue": "^3.3.0",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.345",
    "@iconify/vue": "^5.0.0",
    "@types/node": "^22.15.30",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.8"
  }
}
